<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navi_board"
    app:startDestination="@id/boardFragment">

    <fragment
        android:id="@+id/boardFragment"
        android:name="com.example.tita.ui.board.BoardFragment"
        tools:layout="@layout/fragment_board"
        android:label="BoardFragment" >
        <action
            android:id="@+id/action_boardFragment_to_boardFreeFragment"
            app:destination="@id/boardFreeFragment" />
    </fragment>
    <fragment
        android:id="@+id/boardFreeFragment"
        tools:layout="@layout/fragment_board_free"
        android:name="com.example.tita.ui.board.BoardFreeFragment"
        android:label="BoardFreeFragment" >
        <action
            android:id="@+id/action_boardFreeFragment_to_boardFragment"
            app:destination="@id/boardFragment" />
    </fragment>
</navigation>